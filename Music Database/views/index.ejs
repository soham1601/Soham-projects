<!DOCTYPE html>
<head>
    <title>
        SONG DETAILS
    </title>
    <style>
        th{
         border: 1px solid black;
        }
    </style>
</head>
<body>
    <table cellspacing="8" style="border: 1px solid black;">
        <tr>
            <th>
                Song Name
            </th>
            <th>
                Film Name
            </th>
            <th>
                Music Director
            </th>
            <th>
                Singer
            </th>
            <th>
                Actor
            </th>
            <th>
                Actress
            </th>
	   </tr>
       <% for(var i=0; i<data.length; i++) {%>
        <tr>
          <td>
            <%= data[i].songName%>
          </td>
          <td>
            <%= data[i].filmName%>
          </td>
          <td>
            <%= data[i].musicDirector%>
          </td>
          <td>
            <%= data[i].singer%>
          </td>
          <td>
            <%= data[i].actor%>
          </td>
          <td>
            <%= data[i].actress%>
          </td>

        </tr>
     <% } %>
    </table>
</body>

</html>